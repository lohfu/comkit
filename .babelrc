{
  "env": {
    "node": {
      "presets": [ "es2015-node6", "es2016", "es2017", "stage-0" ],
      "plugins": [
        "add-module-exports",
        ["transform-react-jsx", { "pragma": "h" }],
        ["jsx-node/babel/alias", {
          "lowline": "lodash",
          "preact": "jsx-node",
          "mini-qs": "querystring"
        }],
        ["jsx-node/babel/stringify"]
      ]
    }
  }
}

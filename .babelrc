{
  "env": {
    "node": {
      "presets": [ "es2015-node6", "es2016", "es2017", "stage-0" ],
      "plugins": [
        "add-module-exports",
        ["transform-react-jsx"],
        ["module-resolver", {
          "alias": {
            "lowline": "lodash",
            "mini-qs": "querystring"
          }
        }]
      ]
    }
  }
}
